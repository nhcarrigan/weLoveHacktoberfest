{
  "name": "we-love-matt",
  "version": "4.2.0",
  "description": "A bot to show Hacktoberfest some love",
  "main": "./prod/index.js",
  "scripts": {
    "prebuild": "rimraf ./prod && prisma generate",
    "build": "tsc",
    "start": "node -r dotenv/config ./prod/index.js",
    "lint": "eslint src --max-warnings 0 && prettier src --check",
    "pretest": "rimraf ./.nyc_output && ts-node test/validateCoverage.ts",
    "test": "nyc --check-coverage ts-mocha -u tdd  'test/**/*.spec.ts' --recursive --exit --timeout 10000",
    "test:nocov": "MOCHA=true ts-mocha -u tdd  'test/**/*.spec.ts' --recursive --exit --timeout 10000",
    "posttest": "ts-node test/validateCoverage.ts --post-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/we-love-matt.git"
  },
  "author": "Naomi Carrigan",
  "license": "SEE LICENSE IN https://docs.nhcarrigan.com/#/license",
  "bugs": {
    "url": "https://github.com/nhcarrigan/we-love-matt/issues"
  },
  "homepage": "https://github.com/nhcarrigan/we-love-matt#readme",
  "engines": {
    "node": "20",
    "pnpm": "8"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "@sentry/integrations": "7.73.0",
    "@sentry/node": "7.73.0",
    "discord.js": "14.13.0",
    "dotenv": "16.3.1",
    "string-similarity": "4.0.4",
    "typescript": "5.2.2",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@nhcarrigan/eslint-config": "3.2.0",
    "@nhcarrigan/prettier-config": "3.2.0",
    "@nhcarrigan/typescript-config": "3.0.0",
    "@types/chai": "4.3.6",
    "@types/mocha": "10.0.2",
    "@types/node": "20.8.2",
    "@types/string-similarity": "4.0.0",
    "axios": "1.5.1",
    "chai": "4.3.10",
    "eslint": "8.50.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "3.0.3",
    "prisma": "5.3.1",
    "rimraf": "5.0.5",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.1"
  }
}
