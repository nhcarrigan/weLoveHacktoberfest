{
  "name": "we-love-matt",
  "version": "2.2.1",
  "description": "A bot to show Hacktoberfest some love",
  "main": "./prod/index.js",
  "scripts": {
    "prebuild": "rm -rf ./prod",
    "build": "tsc",
    "start": "node -r dotenv/config ./prod/index.js",
    "lint": "eslint src",
    "test": "echo 'No tests at this time. Would you like to contribute some?'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/we-love-matt.git"
  },
  "author": "Nicholas Carrigan",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/nhcarrigan/we-love-matt/issues"
  },
  "homepage": "https://github.com/nhcarrigan/we-love-matt#readme",
  "engines": {
    "node": "^16.2.0",
    "npm": "^7.13.0"
  },
  "dependencies": {
    "@sentry/integrations": "^6.11.0",
    "@sentry/node": "^6.11.0",
    "discord.js": "^13.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "16.4.13",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
